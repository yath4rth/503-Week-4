public class Car {
	String factory, model, colour;
	int year;
	public Car(String factory, String model, int year, String colour)
	{
		this.factory = factory;
		this.model = model;
		this.year = year;
		this.model = model;
	}
	public void carAge()
	{
		System.out.println("Car Age = "+(2024-year));
	}
	public static void main()
	{
		Car car = new Car("BMW", "X5", 2021, "Black");
		System.out.println("Factory = "+car.factory);
		System.out.println("Model = "+car.model);
		System.out.println("Year = "+car.year);
		System.out.println("Colour = "+car.colour);
		car.carAge();
	}
}
